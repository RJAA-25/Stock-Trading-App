<h1>Create Trader Account</h1>
<%= link_to "Cancel", accounts_path %>
<%= render "partials/navigation" %>

<%= form_with(scope: @account, url: accounts_path) do |f| %>

  <div>
    <%= f.label :first_name %><br/>
    <%= f.text_field :first_name %>
  </div>

  <div>
    <%= f.label :last_name %><br/>
    <%= f.text_field :last_name %>
  </div>
  
  <div>
    <%= f.label :email %><br/>
    <%= f.email_field :email %>
  </div>

  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>

  <%= f.submit "Create Trader"%>
<% end %>

